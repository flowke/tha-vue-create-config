{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "awesome app {{packageName}} :) ",
  "main": "index.js",
  "scripts": {
    "start": "tha-v dev",
    "build": "tha-v build"
  },
  "dependencies": {
    {% spaceless %}
    {% for key, val in dependencies %}
      {% if loop.last %}"
      {{key}}": "{{val}}"
      {% else loop.last %}
      "{{key}}": "{{val}}",
      {% endif %}
    {% endfor %}
     {% endspaceless %}
  },
  "devDependencies": {

  }
}